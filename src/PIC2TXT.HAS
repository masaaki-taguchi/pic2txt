	move.w	$e80028,d0
	lsr.w	#8,d0
	move.w	d0,$e82400

	movea.l	#$eb080a,a1
	move.w	(a0)+,$2(a1)		*H-DISPの設定
	move.w	#$ff,d0
	dbra	d0,*			*130μsec 以上待つ

	move.w	(a0)+,(a1)		*H-TOTAL
	move.w	(a0)+,$4(a1)		*V-DISP
	move.w	$e80028,d0
	ext.w	d0
	move.w	d0,$6(a1)		*$eb0810
