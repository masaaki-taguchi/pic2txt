change_crt_mode:
	movem.l	d0-d1/a0-a2,-(sp)

	move.w	(a0)+,d0
	move.b	d0,$e8e007		*HRLの設定

	move.w	(a0),d0
	movea.l	#$e80000,a1
	move.w	$28(a1),d1
